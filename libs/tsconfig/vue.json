{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vue.js",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"]
  },
  "include": ["env.d.ts", "src/**/*"]
}
